{
  "preInstallUrls":[
    "oakpaltest:/target/test-packages/tmp_foo_bar.zip"
  ],
  "repoInitUrls": [
    "oakpaltest:/target/test-classes/OpearPackageMojoTest/repoinit1.txt",
    "oakpaltest:/target/test-classes/OpearPackageMojoTest/repoinit2.txt"
  ],
  "forcedRoots":[
    {
      "path":"/acme:test"
    }
  ],
  "jcrNodetypes":{
    "oakpal:SimpleType":{
      "@":[
        "mixin"
      ]
    },
    "sling:Folder":{
      ">":[
        "nt:base",
        "nt:folder"
      ],
      "-":[
        {
          "name":"*",
          "type":"undefined",
          "@":[
            "multiple"
          ]
        },
        {
          "name":"*",
          "type":"undefined"
        }
      ],
      "+":[
        {
          "name":"*",
          "types":[
            "nt:base"
          ],
          "=":"sling:Folder",
          "@":[
            "version"
          ]
        }
      ]
    },
    "sling:HierarchyNode":{
      ">":[
        "nt:hierarchyNode"
      ],
      "@":[
        "mixin"
      ]
    },
    "sling:Mapping":{
      ">":[
        "nt:base",
        "nt:hierarchyNode",
        "sling:MappingSpec",
        "sling:Resource"
      ],
      "@":[
        "orderable"
      ],
      "+":[
        {
          "name":"*",
          "types":[
            "nt:base"
          ],
          "=":"sling:Mapping",
          "@":[
            "version"
          ]
        }
      ]
    },
    "sling:MappingSpec":{
      "@":[
        "mixin"
      ],
      "-":[
        {
          "name":"sling:internalRedirect",
          "type":"String"
        },
        {
          "name":"sling:internalRedirect",
          "type":"String",
          "@":[
            "multiple"
          ]
        },
        {
          "name":"sling:match",
          "type":"String"
        },
        {
          "name":"sling:redirect",
          "type":"String"
        },
        {
          "name":"sling:status",
          "type":"Long"
        }
      ]
    },
    "sling:OrderedFolder":{
      ">":[
        "nt:base",
        "sling:Folder"
      ],
      "@":[
        "orderable"
      ],
      "+":[
        {
          "name":"*",
          "types":[
            "nt:base"
          ],
          "=":"sling:OrderedFolder",
          "@":[
            "version"
          ]
        }
      ]
    },
    "sling:Redirect":{
      ">":[
        "sling:Resource"
      ],
      "@":[
        "mixin"
      ],
      "-":[
        {
          "name":"sling:target",
          "type":"undefined"
        }
      ]
    },
    "sling:Resource":{
      "@":[
        "mixin"
      ],
      "-":[
        {
          "name":"sling:resourceType",
          "type":"String"
        }
      ]
    },
    "sling:ResourceAlias":{
      "@":[
        "mixin"
      ],
      "-":[
        {
          "name":"sling:alias",
          "type":"String",
          "@":[
            "multiple"
          ]
        },
        {
          "name":"sling:alias",
          "type":"String"
        }
      ]
    },
    "sling:ResourceSuperType":{
      "@":[
        "mixin"
      ],
      "-":[
        {
          "name":"sling:resourceSuperType",
          "type":"String"
        }
      ]
    },
    "sling:VanityPath":{
      "@":[
        "mixin"
      ],
      "-":[
        {
          "name":"sling:redirect",
          "type":"Boolean"
        },
        {
          "name":"sling:redirectStatus",
          "type":"Long"
        },
        {
          "name":"sling:vanityOrder",
          "type":"Long"
        },
        {
          "name":"sling:vanityPath",
          "type":"String",
          "@":[
            "multiple"
          ]
        }
      ]
    }
  },
  "jcrPrivileges":[
    "acme:meepMeep"
  ],
  "jcrNamespaces":[
    {
      "prefix":"acme",
      "uri":"http://www.acme.com/"
    },
    {
      "prefix":"sling",
      "uri":"http://sling.apache.org/jcr/sling/1.0"
    },
    {
      "prefix":"oakpal",
      "uri":"http://adamcin.net/ns/jcr/oakpal/1.0"
    }
  ]
}